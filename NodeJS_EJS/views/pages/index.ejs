<!DOCTYPE html>
<html lang="pt-br">

    <%- include('../partials/head'); %> <!--Link no formato ejs-->

    <body class="container p-5">

        <%- include('../partials/header'); %>

        <main>
            <div class="container-fluid bg-dark text-white p-5">
                <h1>Node com EJS</h1>
                <p><%= subtitle %></p>
                <p style="color:lightgreen">Vamos aprender a trabalhar com arquivos EJS</p>
            </div>
            <ul class="pt-3">
                <% qualitys.forEach(function(quality){ %>
                    <li>
                        <strong><%= quality.title %></strong> <%= quality.message %>
                    </li>
                <% }) %>
            </ul>
        </main>
        
        <%- include('../partials/footer'); %>
        
    </body>
</html>